<?xml version="1.0"?>
<!--
  `stx:call-procedure' must be a descendant of stx:template or stx:procedure
-->
<stx:transform xmlns:stx="http://stx.sourceforge.net/2002/ns" version="1.0">

  <stx:variable name="v">
    <stx:call-procedure name="p" />
  </stx:variable>

  <stx:procedure name="p">
    <stx:process-children />
  </stx:procedure>
  
</stx:transform>
