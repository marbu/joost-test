<?xml version="1.0"?>
<!--
9: 'stx:choose' may only contain stx:when and stx:otherwise children (encountered 'stx:if')
-->
<stx:transform xmlns:stx="http://stx.sourceforge.net/2002/ns" version="1.0">

  <stx:template match="/">
    <stx:choose>
      <stx:if test="true()" />
      <stx:otherwise/>
    </stx:choose>
  </stx:template>

</stx:transform>
