<?xml version="1.0"?>
<!-- The parameter passed to the namespace-uri function must be a node
     (got number 1.0) -->
<stx:transform xmlns:stx="http://stx.sourceforge.net/2002/ns" version="1.0">
  <stx:template match="/">
    <stx:for-each select="(1,2)">
      <stx:value-of select="namespace-uri()" />
    </stx:for-each>
  </stx:template>
</stx:transform>
