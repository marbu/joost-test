<?xml version="1.0"?>
<!-- The identity transformation: default copy for attributes -->
<stx:transform xmlns:stx="http://stx.sourceforge.net/2002/ns" version="1.0"
               pass-through="all">

  <stx:template match="*">
    <stx:copy>
      <stx:process-attributes />
      <stx:process-children />
    </stx:copy>
  </stx:template>
  
</stx:transform>
