<?xml version="1.0"?>
<!-- rename example -->
<stx:transform xmlns:stx="http://stx.sourceforge.net/2002/ns" version="1.0"
               pass-through="text">

  <stx:template match="root">
    <stx:copy>
      <stx:process-children />
    </stx:copy>
  </stx:template>

  <stx:template match="data">
    <foo>
      <stx:process-children />
    </foo>
  </stx:template>

</stx:transform>
