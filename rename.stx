<?xml version="1.0"?>
<!-- rename example -->
<stx:transform xmlns:stx="http://temporary.uri/2002/stx" version="1.0">

  <stx:options no-match-events="text" />

  <stx:template match="root">
    <stx:copy>
      <stx:process-children />
    </stx:copy>
  </stx:template>

  <stx:template match="data">
    <foo>
      <stx:process-children />
    </foo>
  </stx:template>

</stx:transform>
